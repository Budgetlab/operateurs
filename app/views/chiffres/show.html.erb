<main role="main">
  <div class="fr-container">
    <h1 class="fr-my-4w title-btn"><%= @organisme.nom %>
      <% if @organisme.acronyme && !@organisme.acronyme.blank? %> - <%= @organisme.acronyme %>
      <% end %></h1>
    <% if @chiffre.statut == "valide" %>
      <div class="fr-mb-4w">
        <p class="fr-badge fr-badge--pink-tuile"><%= @chiffre.type_budget %> <%= @chiffre.exercice_budgetaire %></p>
        <p class="fr-badge
          <% if @chiffre.phase == 'pré-CA' %>fr-badge--brown-caramel
          <% elsif @chiffre.phase == 'CA' %>fr-badge--green-archipel
          <% else %>fr-badge--blue-ecume
          <% end %>"><%= @chiffre.phase %></p>
        <% if @chiffre.operateur == true %><p class="fr-badge fr-badge--info fr-badge--no-icon">Opérateur</p>
        <% else %><p class="fr-badge fr-badge--beige-gris-galet">Non opérateur</p>
        <% end %>
        <% if @chiffre.comptabilite_budgetaire == true %>
          <p class="fr-badge fr-badge--success fr-badge--no-icon">Comptabilité budgétaire</p>
        <% else %><p class="fr-badge fr-badge--purple-glycine">Hors comptabilité budgétaire</p>
        <% end %>
      </div>
      <hr class="fr-my-2w">

      <div data-tab-target="section" data-tab-value="emplois" class="fr-mb-4w">
        <h2 class="title-btn">Emplois</h2>
        <h3 class="fr-h4">Emplois rémunérés par l’organisme</h3>
        <% title_emplois = @chiffre.comptabilite_budgetaire ? 'autorisation d’emplois' : 'emplois' %>
        <% title_depenses_or_charges =  @chiffre.comptabilite_budgetaire ? 'dépenses' : 'charges'%>
        <% if @chiffre.operateur == true %>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="fr-capitalize"><%= title_emplois %> sous plafond LFI</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_plafond) %> ETPT</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="fr-capitalize"><%= title_emplois %> hors plafond LFI</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_hors_plafond) %> ETPT</div>
            </div>
            <div class="fr-col-6">
              <div>
                <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Hors-plafond / total effectifs rémunérés par l'organisme</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.ratio_hors_plafond) %> %</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--grey fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Total des <%= title_emplois %> rémunérés par l'organisme</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_decimal(@chiffre.emplois_total) %> ETPT</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        <%else %>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div>Total des <%= title_emplois %> rémunérés par l'organisme</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_total) %> ETPT</div>
            </div>
          </div>
        <% end %>
        <div class="fr-grid-row fr-grid-row--gutters">
          <div class="fr-col-6">
            <% if @chiffre.comptabilite_budgetaire == true %>
              <div>Total des dépenses de personnel</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.emplois_depenses_personnel) %> €</div>
            <%else %>
              <div>Total des charges de personnel</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.emplois_charges_personnel) %> €</div>
            <%end %>
          </div>
          <div class="fr-col-6">
            <div>
              <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                <div class="fr-cart__body">
                  <p class="fr-mb-0 fr-text--center">Coût moyen par ETPT</p>
                  <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.cout_moyen_etpt) %> €</p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <% if @chiffre.operateur == true %>
          <h3 class="fr-h4 fr-mt-4w options ">Rappel du plafond </h3>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-12">
              <div>Rappel du plafond d'emplois inscrit en PLF / LFI</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_plafond_rappel) %> ETPT</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-12">
              <div>Plafond d'emplois pré-notifié à l'organisme par le RPROG</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_plafond_prenotifie) %> ETPT</div>
            </div>
          </div>
          <h3 class="fr-h4 fr-mt-4w">Schéma d’emplois</h3>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-12">
              <div>Schéma d'emplois</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre(@chiffre.emplois_schema) %> ETP</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-12">
              <div>Schéma d'emplois pré-notifié à l'organisme par le RPROG</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre(@chiffre.emplois_schema_prenotifie) %> ETP</div>
            </div>
          </div>
        <% end  %>
        <h3 class="fr-h4 fr-mt-4w options">Répartition des emplois et <%= title_depenses_or_charges %> de personnel par statut</h3>
        <div class="fr-grid-row fr-grid-row--gutters options">
          <div class="fr-col-6">
            <div>Total des emplois titulaires</div>
            <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_titulaires) %> ETPT</div>
          </div>
        </div>
        <div class="fr-grid-row fr-grid-row--gutters options">
          <div class="fr-col-6">
            <div class="fr-capitalize"><%= title_depenses_or_charges %> de personnel des emplois titulaires</div>
            <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.emplois_titulaires_montant) %> €</div>
          </div>
          <div class="fr-col-6">
            <div>
              <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                <div class="fr-cart__body">
                  <p class="fr-mb-0 fr-text--center">Coût moyen des emplois titulaires</p>
                  <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.cout_moyen_titulaires) %> €</p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="fr-grid-row fr-grid-row--gutters options">
          <div class="fr-col-6">
            <div>Total des emplois contractuels</div>
            <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_contractuels) %> ETPT</div>
          </div>
          <div class="fr-col-6">
            <div>
              <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                <div class="fr-cart__body">
                  <p class="fr-mb-0 fr-text--center">Part des contractuels dans le total des emplois rémunérés par l’organisme</p>
                  <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.part_contractuels) %> %</p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="fr-grid-row fr-grid-row--gutters options">
          <div class="fr-col-6">
            <div class="fr-capitalize"><%= title_depenses_or_charges %> de personnel des emplois contractuels</div>
            <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.emplois_contractuels_montant) %> €</div>
          </div>
          <div class="fr-col-6">
            <div>
              <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                <div class="fr-cart__body">
                  <p class="fr-mb-0 fr-text--center">Coût moyen des emplois contractuels</p>
                  <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.cout_moyen_contractuels) %> €</p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <h3 class="fr-h4 fr-mt-4w">Autres emplois en fonction dans l’organisme</h3>
        <div class="fr-grid-row fr-grid-row--gutters">
          <div class="fr-col-6">
            <div>Emplois en fonction dans l'organisme, non rémunérés par lui - emplois T2, MAD entrantes remboursées ou non</div>
            <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_non_remuneres) %> ETPT</div>
          </div>
        </div>
        <div class="fr-grid-row fr-grid-row--gutters options">
          <div class="fr-col-6">
            <div>Emplois en fonction dans une autre entité, rémunérés par l'organisme et décomptés dans son plafond d'autorisation d'emplois - MAD sortantes</div>
            <div class="fr-text--bold fr-mt-1w"><%= format_nombre_decimal(@chiffre.emplois_autre_entite) %> ETPT</div>
          </div>
          <div class="fr-col-6">
            <div>
              <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                <div class="fr-cart__body">
                  <p class="fr-mb-0 fr-text--center">Part des personnels exerçant leurs missions dans l’organisme dans le total des emplois rémunérés directement par l’organisme</p>
                  <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.indicateur_part_personnel_organisme) %> %</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <% if @chiffre.comptabilite_budgetaire == true %>
        <hr class="fr-my-2w">
        <div data-tab-target="section" data-tab-value="credits" class="fr-mb-4w">
          <h2 class="title-btn">Crédits </h2>
          <h3 class="fr-h4 fr-mt-4w">Dépenses</h3>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div>Total des autorisations d’engagement (AE)</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_ae_total) %> €</div>
            </div>
            <div class="fr-col-6">
              <div>Total des crédits de paiement (CP)</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_cp_total) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--grey fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Dont total des dépenses de personnel (AE)</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.emplois_depenses_personnel) %> €</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="fr-col-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--grey fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Dont total des dépenses de personnel (CP)</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.emplois_depenses_personnel) %> €</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6 fr-col-offset-lg-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Poids relatif des dépenses de personnel</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.indicateur_poids_depenses_personnel) %> %</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="options">
                <div>Dont total des dépenses de fonctionnement (AE)</div>
                <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_ae_fonctionnement) %> €</div>
              </div>
            </div>
            <div class="fr-col-6">
              <div>Dont total des dépenses de fonctionnement (CP)</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_cp_fonctionnement) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options ">
            <div class="fr-col-6 fr-col-offset-lg-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Part relative des dépenses de fonctionnement (CP) dans le total des dépenses (CP)</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.part_cp_fonctionnement) %> %</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="options">
                <div>Dont total des dépenses d'intervention (AE)</div>
                <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_ae_intervention) %> €</div>
              </div>
            </div>
            <div class="fr-col-6">
              <div>Dont total des dépenses d'intervention (CP)</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_cp_intervention) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-6 fr-col-offset-lg-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Part relative des dépenses d'intervention (CP) dans le total des dépenses (CP)</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.part_cp_intervention) %> %</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="options">
                <div>Dont total des dépenses d'investissement (AE)</div>
                <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_ae_investissement) %> €</div>
              </div>
            </div>
            <div class="fr-col-6">
              <div>Dont total des dépenses d'investissement (CP)</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_cp_investissement) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-6 fr-col-offset-lg-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Part relative des dépenses d'investissement (CP) dans le total des dépenses (CP)</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.part_cp_investissement) %> %</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-6">
              <div>Total des crédits de paiement ouverts en N pour des opérations pluriannuelles</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_cp_operations) %> €</div>
            </div>
            <div class="fr-col-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--blue fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Poids des crédits de paiement au titre d'opérations pluriannuelles</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.poids_cp_operations) %> %</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters options">
            <div class="fr-col-6">
              <div>Total des dépenses sur recettes fléchées (en CP)</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_cp_recettes_flechees) %> €</div>
            </div>
          </div>

          <h3 class="fr-h4 fr-mt-4w">Recettes</h3>
          <% if @chiffre.operateur == true %>
            <div class="fr-grid-row fr-grid-row--gutters">
              <div class="fr-col-6">
                <div>Subvention pour charges de service public</div>
                <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_subvention_sp) %> €</div>
              </div>
            </div>
            <div class="fr-grid-row fr-grid-row--gutters">
              <div class="fr-col-6">
                <div>Subvention pour charges d'investissement globalisée</div>
                <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_subvention_investissement_globalisee) %> €</div>
              </div>
              <div class="fr-col-6">
                <div>Subvention pour charges d'investissement fléchée</div>
                <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_subvention_investissement_flechee) %> €</div>
              </div>
            </div>
          <% end  %>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div>Autres financements de l’Etat</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_financements_etat_autres) %> €</div>
            </div>
            <div class="fr-col-6">
              <div>Autres financements de l’Etat fléchés</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_financements_etat_fleches) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div>Fiscalité affectée</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_fiscalite_affectee) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div>Autres financements publics</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_financements_publics_autres) %> €</div>
            </div>
            <div class="fr-col-6">
              <div>Autres financements publics fléchés</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_financements_publics_fleches) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div>Recettes propres globalisées</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_recettes_propres_globalisees) %> €</div>
            </div>
            <div class="fr-col-6">
              <div>Recettes propres fléchées</div>
              <div class="fr-text--bold fr-mt-1w"><%= format_nombre_entier(@chiffre.credits_recettes_propres_flechees) %> €</div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--grey fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Total des recettes globalisées</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.recettes_globalisees) %> €</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="fr-col-6">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--grey fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Total des recettes fléchées</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.recettes_flechees) %> €</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
            <div class="fr-col-12">
              <div class="fr-mb-2w">
                <div class="fr-card fr-card--grey fr-p-2w fr-card--no-border">
                  <div class="fr-cart__body">
                    <p class="fr-mb-0 fr-text--center">Total des recettes</p>
                    <p class="fr-text--bold fr-mb-0 fr-text--center"><%= format_nombre_entier(@chiffre.recettes_total) %> €</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="fr-grid-row fr-grid-row--gutters">
